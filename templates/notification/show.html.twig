{% extends 'base.html.twig' %}

{% block title %}Notification{% endblock %}

{% block body %}
    <h1>Notification</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ notification.id }}</td>
            </tr>
            <tr>
                <th>Object</th>
                <td>{{ notification.object }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ notification.content }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ notification.statut }}</td>
            </tr>
            <tr>
                <th>CreationDate</th>
                <td>{{ notification.creationDate ? notification.creationDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>ModifDate</th>
                <td>{{ notification.modifDate ? notification.modifDate|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_notification_index') }}">back to list</a>

    <a href="{{ path('app_notification_edit', {'id': notification.id}) }}">edit</a>

    {{ include('notification/_delete_form.html.twig') }}
{% endblock %}

{% extends 'base.html.twig' %}

{% block title %}Region{% endblock %}

{% block body %}
<main>
    <h1>Region</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ region.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ region.Name }}</td>
            </tr>
            <tr>
                <th>RegionStatut</th>
                <td>{{ region.regionStatut ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_region_index') }}">back to list</a>

    <a href="{{ path('app_region_edit', {'id': region.id}) }}">edit</a>

    {{ include('region/_delete_form.html.twig') }}
</main>
{% endblock %}
